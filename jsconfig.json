{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
				// name and path to aliases
			"@lib": ["src/lib"],  
			"$lib":["src/lib"],
  			"$lib/*":["src/lib/*"],
			"@stores": ["src/stores.js"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
